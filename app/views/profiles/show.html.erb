<h1>Your Profile</h1>

<%= @profile.username %>
<% @profile.shouts.each do |s| %>
<ul>
  <li><%= link_to "#{s.title}", "/profiles/#{@profile.id}/shouts/#{s.id}"%></li>
</ul>
<% end %>

<%= link_to "Back to Shouts!", profile_shouts_path(@profile) %>

<%= link_to "Edit Profile", edit_profile_path(@profile) %>

<%= link_to "Logout", logout_path, method: :post %> 