

<div class="col-xs-4"
  <h1>ShoutOut!</h1>
</div>
<div class= "box">

<%= form_tag login_path  do %>


  <div><%= label_tag(:username)%></div>
  <div><%= text_field_tag(:username, params[:profile], autofocus: true)%></div>


  <div><%= label_tag(:password)%></div>
  <div><%= password_field_tag(:password)%></div>

  <div><%= submit_tag("Log in", class: "btn btn-primary") %></div>

<% end %>
</div>
<h3>Are you new? Let's get you a profile!</h3>
<%= link_to "New Profile", new_profile_path %>

<br><br>
<%= link_to "About Us", '/site/about' %>



