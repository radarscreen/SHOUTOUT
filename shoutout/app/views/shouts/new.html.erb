<h1>Shout It!</h1>

<%= form_for(@shout, url: profiles_path) do |f|%>


  <%= f.label :title  %>
  <%= f.text_field :title, autofocus: true %>

  <%= f.label :time %>
  <%= f.text_field :time %>


  <%= f.label :location %>
  <%= f.text_field :location  %>

  <%= f.label :description %>
  <%= f.text_area :description  %>


  <%= f.submit("Shout Out!", class: "btn btn-primary") %>
<% end %>
<br>
<%= link_to "Back to Shouts Page", :back %>